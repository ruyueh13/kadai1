<p id="notice"><%= notice %></p>

<h1>Alls</h1>

<table>
  <thead>
    <tr>
      <th>Top</th>
      <th>Text</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @alls.each do |all| %>
      <tr>
        <td><%= all.top %></td>
        <td><%= all.text %></td>
        <td><%= link_to 'Show', all %></td>
        <td><%= link_to 'Edit', edit_all_path(all) %></td>
        <td><%= link_to 'Destroy', all, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New All', new_all_path %>
