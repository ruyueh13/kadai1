<p id="notice"><%= notice %></p>

<h1>Message1s</h1>

<table>
  <thead>
    <tr>
      <th>Title1</th>
      <th>Message1</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @message1s.each do |message1| %>
      <tr>
        <td><%= message1.title1 %></td>
        <td><%= message1.message1 %></td>
        <td><%= link_to 'Show', message1 %></td>
        <td><%= link_to 'Edit', edit_message1_path(message1) %></td>
        <td><%= link_to 'Destroy', message1, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Message1', new_message1_path %>
